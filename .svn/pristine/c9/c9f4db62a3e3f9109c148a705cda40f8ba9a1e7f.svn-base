"use.strict";

/*
 * Department Service
 */


app.factory("Department", function (Restangular, alertMsg) {
    return {
        allDepartment: function ($scope, company, uid) {
            Restangular.one('department/all/info/', company).get().then(function(response) {
                alertMsg.alert($scope, response, '');
            })

        },
    }
});
